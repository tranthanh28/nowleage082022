cte laravel
WITH recursive laravel_cte AS ((
                    SELECT
                        user_tree.user_id,
                        user_tree.parent_id
                    FROM
                        user_tree
                    WHERE
                            user_tree.parent_id = {$userId}
                            ) UNION ALL
                    (
                    SELECT
                        user_tree.user_id,
                        user_tree.parent_id
                    FROM
                        user_tree
                        INNER JOIN laravel_cte ON laravel_cte.user_id = user_tree.parent_id
                    )) SELECT
                *
            FROM
                laravel_cte ";
